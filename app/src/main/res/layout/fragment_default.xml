                android:text="Bienvenid@"
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/tv_bienvenida"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Texto de Ejemplo"
                android:textSize="20sp"/>
        </LinearLayout>
    </LinearLayout>

</FrameLayout>